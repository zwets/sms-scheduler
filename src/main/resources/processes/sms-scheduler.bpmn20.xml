        <timeDate>${scheduledTime}</timeDate>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <serviceTask id="sendSmsService" name="SEND" flowable:async="true" flowable:exclusive="false" flowable:type="send-event">
